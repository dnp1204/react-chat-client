{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": ["error", "never"],
    "no-unused-vars": ["warn"],
    "no-var": ["off"],
    "one-var": ["off"],
    "object-curly-newline": "off",
    "no-underscore-dangle": "off",
    "no-shadow": "off",
    "consistent-return": "off",
    "arrow-body-style": "off",
    "func-names": "off",
    "space-before-function-paren": "off",
    "prefer-arrow-callback": "off"
  }
}
